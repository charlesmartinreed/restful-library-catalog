<h2>Search Authors</h2>
<form action="/authors" method="GET">
  <label>Name</label>
  <input type="text" name="author_name" value="<%= searchOptions.name %>" />
  <button type="submit">Search</button>
</form>
<br />
<!-- loop through all the authors -->
<% authors.forEach(author => { %>
<div><%= author.name %></div>
<% }) %>
